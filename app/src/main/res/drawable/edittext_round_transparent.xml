<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_focused="true" android:state_pressed="true">
        <shape>
            <gradient android:angle="270" android:centerColor="#17ffffff" android:endColor="#17ffffff" android:startColor="#17ffffff" />

            <stroke android:width="2.3dp" android:color="#ffa29bbc" />

            <corners android:radius="95dp" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="true">
        <shape>
            <gradient android:angle="270" android:centerColor="#17ffffff" android:endColor="#17ffffff" android:startColor="#17ffffff" />

            <stroke android:width="2.3dp" android:color="#ffa29bbc" />

            <corners android:radius="95dp" />
        </shape>
    </item>
    <item android:state_focused="true" android:state_pressed="false">
        <shape>
            <gradient android:angle="270" android:centerColor="#0Dffffff" android:endColor="#0Dffffff" android:startColor="#0Dffffff" />

            <stroke android:width="0.7dp" android:color="#ffa29bbc" />

            <corners android:radius="95dp" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <gradient android:angle="270" android:centerColor="#0Dffffff" android:endColor="#0Dffffff" android:startColor="#0Dffffff" />

            <stroke android:width="0.7dp" android:color="#ffa29bbc" />

            <corners android:radius="95dp" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
        </shape>
    </item>

</selector>